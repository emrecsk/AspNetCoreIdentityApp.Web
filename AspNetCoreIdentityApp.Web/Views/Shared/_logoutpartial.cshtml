@{
    if (User.Identity!.IsAuthenticated)
    {
        <ul class="navbar-nav">
            <li class="nav-item me-0">
                <a class="nav-link text-dark" href="#">
                    Hello, @User.Identity.Name
                </a>
            </li>
            <li class="nav-item me-0 mt-1"><a class="dropdown-item" asp-controller="Member" asp-action="Index">Profile</a></li>
            <li class="nav-item me-0 mt-1"><a class="dropdown-item" asp-controller="Member" asp-action="PasswordChange">Password Change</a></li>
            <li class="nav-item me-0 mt-1"><a class="dropdown-item" asp-controller="Member" asp-action="UserUpdate">Update User Info</a></li>
            <li class="nav-item me-0 mt-1"><a class="dropdown-item" asp-route-returnurl="/Home/Index" asp-controller="Member" asp-action="LogOut">Sign Out</a></li>
        </ul>
    }
    else
    {
        <ul class="navbar-nav">
            <li class="nav-item me-0"><a class="btn btn-outline-success mx-2" asp-area="" asp-controller="Home" asp-action="SignUp">Sign Up</a></li>
            <li class="nav-item me-0"><a class="btn btn-outline-success" asp-area="" asp-controller="Home" asp-action="SignIn">Sign In</a></li>
        </ul>

    }
}